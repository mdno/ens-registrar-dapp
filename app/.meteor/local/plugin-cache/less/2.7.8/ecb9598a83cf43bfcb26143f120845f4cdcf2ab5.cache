\n}\n\n.dapp-url-bar {\n    display: block;\n    text-align: center;\n    width: 100%;\n    // .calc(width, ~'100% - 64px');    \n    .border-radius(8px);\n}\n\n\n\n.dapp-account-list {\n    @dappPaddingVertical:  @gridHeight * 0.5;\n    @dappPaddinghorizontal:  @gridWidth * 1.5;\n\n    // flex-grow: 2;\n    list-style-type: none;\n    overflow: auto;\n    padding-left: 0;\n    .dapp-vertical-menu;\n\n    li {\n        padding: @dappPaddingVertical/2 0;\n        padding-left: @dappPaddinghorizontal;\n        min-height: @gridHeight * 2.5;\n        position: relative;\n        overflow: hidden;\n        font-family: @fontFamily;\n\n\n        button.selected {\n            min-height: @gridHeight * 2;\n            border-left: 3px solid @colorLink;\n\n            .dapp-identicon {\n                margin-top: -2px;\n                margin-left: -2px; \n                border: 2px solid @colorGray;\n                width: 36px;\n                height: 36px;\n            }\n\n            figure {\n                position: absolute;\n                color: @colorWhite;\n                right: 0;\n                top: 0;\n                font-size: 160%;\n                z-index: 10;\n                bottom: 0;\n                text-align: center;\n                padding-top: 4px;\n                padding: 9px 10px;\n                background: linear-gradient(to right, transparent, @colorLink 50%);\n            }\n        }\n\n        button {\n            position: relative;\n            display: block;\n            display: flex;\n            flex-direction: column;\n            justify-content: center;\n            padding: @dappPaddingVertical/2 0;\n            padding-left: @dappPaddinghorizontal;\n            margin: -@dappPaddingVertical/2 0;\n            margin-left: -@dappPaddinghorizontal;\n            width: 100%;\n            height: 100%;\n            min-height: @gridHeight * 2;\n            font-family: @fontFamily;\n            text-align: left;\n            border-left-color: @colorGray;\n            border-left-style: solid;\n            outline: none;\n            .transition(border 0.25s);\n\n            &:focus {\n                border-bottom: none !important;                \n            }\n\n            &:hover {\n                border-left-width: 6px;\n                border-bottom: none !important;                \n            }\n\n            &:active {\n                transform: scale(1) !important;\n            }\n\n        }\n\n        .dapp-identicon {\n            position: absolute;\n            top: @gridHeight/3;\n            left: @gridWidth/4;\n        }\n\n        h3 {\n            .dapp-shorten-text;\n            display: block;\n            padding: 0;\n            margin: 0;\n            color: @colorLink;\n            text-transform: none;\n            font-weight: 400;\n            font-size: 90%;\n            text-shadow: #fff 0 1px 0;\n        }\n\n        h3 + span {\n            // position: relative;\n            // top: -0.25 * @gridHeight;\n            white-space: nowrap;\n            overflow: hidden;\n            text-overflow: ellipsis;\n            font-size: 75%;\n            opacity: 0.5;\n            color: @colorTextSecondary;\n        }\n    }\n}\n\n\n.dapp-modal-buttons {\n    .display(flex);\n    .justify-content(flex-end);\n    \n    margin: @gridHeight @gridWidth 0;\n\n    button, a, a:visited {\n        .flex(0 0 auto);\n        font-size: 1.1em;\n        max-width: none;\n        margin: @defaultPaddingVertical @defaultPaddingHorizontal;\n        font-weight: 300;\n\n        &.dapp-primary-button {\n            font-weight: 500;\n        }\n\n        &:last-child {\n            margin-right: 0;\n        }\n    }\n}\n\n\n// default elements\n\na,\na:visited,\nbutton {\n    text-decoration: none;\n    color: @colorLink;\n\n    outline: 0;\n\n    &:hover,\n    &:focus {\n        outline: 0;\n    }\n    &:focus {\n        border-bottom: 1px dotted @colorLink;\n    }\n    &:active {\n        .scale(0.95);\n        border-bottom: 0;\n    }\n}\nbutton {\n    .dapp-button-reset;\n    font-weight: inherit;\n}\n\nhr {\n    border: 0;\n    height: 0;\n    margin: @defaultMargin*2 0;\n    background-color: transparent;\n    border-bottom: 1px solid @colorGray; \n}\n\nh1, h2, h3, h4 {\n    clear: both;\n}\n\nh1 {\n    margin: @defaultMargin 0;\n    margin-bottom: @defaultMargin*3;\n    font-weight: 100;\n    font-size: 2.2em;\n    line-height: 1.4em;\n    color: @colorTextSecondary;\n\n    span {\n        font-weight: 500;\n    }\n\n    + h2{\n        margin-top: 0;\n    }\n    + p {\n        margin-top: -@defaultMargin*2;\n    }\n\n    strong {\n        font-weight: 400;\n    }\n}\n\nh2 {\n    display: inline-block;\n    padding: 0 @defaultPaddingHorizontal/4;\n    padding-bottom: 1px;\n    margin: @defaultMargin*3 0;\n    font-size: 1em;\n    text-transform: uppercase;\n    background: @colorTextSecondary;\n    color: @colorWhite;\n\n    font-family: 'Montserrat';\n    font-weight: 400;\n\n    + table {\n        margin-top: 0;\n    }\n}\n\nh3 {\n    margin: @defaultMargin 0;\n    padding: 0;\n    color: fadeout(@colorTextSecondary, 30%);\n    text-transform: uppercase;\n    font-weight: 500;\n    font-size: 1em;\n}\n\nh4 {\n    margin: @defaultMargin 0;\n    padding: 0;\n    color: fadeout(@colorTextSecondary, 30%);\n    font-weight: 500;\n    font-size: 1em;\n}\n\npre,\ncode {\n    .dapp-overflow;\n    word-break: break-word;\n    padding: @defaultPaddingVertical/2;\n    background-color: @colorGrayLight;\n    font-size: 0.8em;\n    text-align: left;\n}\n\n\ntable {\n    width: 100%;\n    margin: @defaultMargin 0;\n\n    &.dapp-zebra {\n        tbody {\n            tr {\n                &:nth-child(odd) {\n                    background-color: fadeout(@colorGray, 70%);\n                }\n            }\n        }\n        thead + tbody {\n            tr {\n                &:nth-child(even) {\n                    background-color: fadeout(@colorGray, 70%);\n                }\n            }\n        }\n    }\n\n    &.dapp-small {\n        td {\n            padding: @defaultPaddingVertical/2 @defaultPaddingHorizontal/4;\n            font-size: 0.8em;\n        }\n    }\n\n    + h2,\n    + h3 {\n        margin-top: @defaultMargin*2;\n    }\n\n    td {\n        padding: @defaultPaddingVertical @defaultPaddingHorizontal/2;\n\n        h2 {\n            margin: 0;\n            padding: 0;\n            background-color: transparent;\n            color: @colorTextSecondary;\n            text-transform: none;\n            font-style: normal;\n        }\n        p {\n            padding-top: @defaultMargin/2;\n            margin: 0;\n            font-size: 0.9em;\n        }\n    }\n\n    thead {\n        background-color: @colorTextSecondary;\n        color: @colorWhite;\n        text-transform: uppercase;\n    }\n}\n\n\n// OVERWRITES for elements from the ethereum:elements package\n.dapp-input {\n    border: 0;\n    border-bottom: solid 2px @colorGray;\n    background-color: @colorGrayLight;\n    color: @colorLink;\n    .transition(opacity 1s);\n\n    .placeholder(@colorGray);\n\n    &:disabled {\n        color: @colorGrayDark;\n        opacity: 0.25;\n    }\n\n    &.dapp-tiny {\n        font-size: 0.6em;\n        border-bottom-width: 1px; \n    }\n    &.dapp-small {\n        font-size: 0.8em;\n    }\n    &.dapp-large {\n        font-size: 1.5em;\n    }\n\n    &:invalid,\n    &.dapp-error {\n        color: @colorError;\n        background: fadeout(@colorError, 90%);\n        border-color: fadeout(@colorError, 85%);\n    }\n}\n\n.dapp-address-input {\n    input {\n        .dapp-input;\n        \n    }\n}\n\n// forms\ninput,\nselect,\ntextarea {\n    .dapp-input;\n    display: inline-block;\n    width: 300px;\n    max-width: 100%;\n    margin-top: @gridHeight;\n    padding: @defaultPaddingVertical/2 @defaultPaddingHorizontal/2;\n    padding-bottom: @defaultPaddingVertical/3;\n    .box-sizing(border-box);\n    .border-radius(0);\n    font-size: 1em;\n    font-weight: 300;\n\n    &:focus {\n        outline: 0;\n        border-color: @colorLink;\n    }\n}\n\nh1 + input,\nh1 + select,\nh1 + textarea,\nh2 + input,\nh2 + select,\nh2 + textarea,\nh3 + input,\nh3 + select,\nh3 + textarea,\nh4 + input,\nh4 + select,\nh4 + textarea {\n    margin-top: 0;\n}\n\n:disabled {\n    color: @colorGrayDark;\n    // background-color: @colorGrayLight;\n}\ninput[type=\"checkbox\"], input[type=\"radio\"] {\n    display: inline-block;\n    position: relative;\n    margin: 0;\n    outline: none !important;\n    -webkit-appearance: none;\n    \n    width: auto;\n\n    width:  24px;\n    height: 24px;\n\n    // Radio\n    &::before {\n        content: '';\n        position: relative;\n        top:  0;\n        left: 0;\n        display: block;\n\n        background: @colorGrayLight;\n        border: 1px solid @colorGrayLight;\n        .box-shadow(inset 0 0 2px rgba(0,0,0,0.2));\n        // .dapp-shadow-medium;\n\n        width:  24px;\n        height: 24px;\n    }\n\n    &:active::before {\n        // .dapp-shadow-medium;\n    }\n\n    &:focus::before {\n        border-color: fadeout(@colorLink, 60%);\n        // .dapp-shadow-medium;\n    }\n    &:disabled::before {\n        cursor: not-allowed;\n        background-color: fadeout(@colorGrayLight, 20%);\n        border-color: @colorGrayLight;\n    }\n\n\n    // Radio Pip\n    &:after {\n        content: '';\n        display: inline-block;\n        position: absolute;\n        top: 6px;\n        left: 6px;\n\n        background: @colorLink;\n        .dapp-shadow-small;\n\n        width:  12px;\n        height: 12px;\n\n        .transition(transform @animationSpeed);\n        .scale(0);\n    }\n\n    &:checked:after {\n        .scale(1);\n    }\n\n    &:disabled:after {\n        background: fadeout(@colorLink, 60%);\n    }\n}\n\ninput[type=\"radio\"] {\n    .border-radius(50%);\n    \n    &:before {\n        .border-radius(50%);\n    }\n    &:after {\n        .border-radius(50%);\n    }\n}\ninput[type=\"range\"] {\n    -webkit-appearance: none;\n    padding: 0;\n    border: 0;\n    background-color: transparent;\n\n    &::-webkit-slider-runnable-track {\n        height: 5px;\n        background-color: @colorGray;\n        border: none;\n        .border-radius(3px);\n    }\n\n    &::-webkit-slider-thumb {\n        -webkit-appearance: none;\n        border: none;\n        height: 16px;\n        width: 16px;\n        border-radius: 50%;\n        background-color: @colorGrayDark;\n        margin-top: -6px;\n    }\n\n    &:focus {\n        outline: none;\n    }\n\n    &:focus::-webkit-slider-thumb {\n        background-color: @colorLink;\n    }\n\n    &::-moz-range-track {\n        // width: 300px;\n        height: 5px;\n        background-color: @colorGray;\n        border: none;\n        .border-radius(3px);\n    }\n\n    &::-moz-range-thumb {\n        -webkit-appearance: none;\n        border: none;\n        height: 16px;\n        width: 16px;\n        border-radius: 50%;\n        background-color: @colorGrayDark;\n        margin-top: -4px;\n    }\n\n    &:focus {\n        outline: none;\n    }\n\n    &:focus::-moz-range-thumb {\n        background-color: @colorLink;\n    }\n}\n\nlabel {\n    font-weight: 300;\n}\nfieldset {\n    border: 0;\n    padding: 0;\n    margin: @defaultMargin;\n}\nselect {\n    height: 45px;\n}\n\n// turns radio buttons in segmented controls\n.dapp-segmented-control {\n    .display(flex);\n\n    &.dapp-small {\n        label {\n            padding: @defaultPaddingVertical/2 @defaultPaddingHorizontal/2;\n            font-size: 0.8em;\n        }\n    }\n\n    input {\n        display: none;\n    }\n\n    label {\n        .flex(1);\n        color: @colorLink;\n        background: @colorGrayLight;\n        padding: @defaultPaddingVertical @defaultPaddingHorizontal;\n        margin: 1px;\n        text-align: center;\n        text-transform: uppercase;\n        font-weight: 400;\n        border-right-width: 0;\n        cursor: pointer;\n        .transition(background-color @animationSpeed/2);\n\n        &:nth-child(2) {\n            .border-radius(4px 0 0 4px);\n        }        \n\n        &:last-child {\n            border-right-width: 1px;\n            .border-radius(0 4px 4px 0);\n        }\n    }\n\n    input:checked + label {\n        background: @colorGray;\n        border-color: @colorGray;\n        color: @colorGrayDark;\n    }\n\n\n}\n","// ==========================================================================\n// Base  Mixins\n// ==========================================================================\n\n// Font Sizes (REMs with PX fallback for older browsers)\n.font-size (@size) {\n  @remValue: @size / @default-font-size;\n  @pxValue: (@size);\n  font-size: ~\"@{pxValue}px\"; \n  font-size: ~\"@{remValue}rem\";\n}\n\n// Line Height Sizes (REMs with PX fallback for older browsers)\n.line-height (@size) {\n  @remValue: @size / @default-font-size;\n  @pxValue: (@size);\n  line-height: ~\"@{pxValue}px\"; \n  line-height: ~\"@{remValue}rem\";\n}\n\n// Rounded Corners\n.border-radius(@radius) {\n  -webkit-border-radius: @radius;\n  -moz-border-radius: @radius;\n  border-radius: @radius;\n}","// colors\n@colorWhite: #FAFAFA;\n@colorBlack: #111;\n@colorBlue: #0e73b8;\n@colorOrange: #ff9800;\n// @colorYellow: #ffc107;\n@colorGreen: #8bc34a;\n// @colorWhiteTransparent: rgba(255, 255, 255, 0.95);\n@colorGray: #CCC6C6;\n@colorGrayLight: lighten(#ecebe6, 4%);\n@colorGrayDark: darken(@colorGray, 40%);\n\n@colorError: #C20E25;\n\n@colorBackgroundOverlay: transparent; // will use blur in ethereum dapps\n@colorBackgroundHeader: lighten(#d9d0d0, 5%);\n//@colorBackgroundAside: #05669A;\n@colorBackgroundAside: @colorGray;\n@colorForegroundAside: @colorBlack;\n\n@colorBackgroundFooter: @colorGrayDark;\n\n@colorBackgroundMainButton: #665F5F;\n@colorForegroundMainButton: @colorWhite;\n\n\n\n@colorTextPrimary: @colorBlack;\n@colorTextSecondary: #827a7a;\n@colorTextTertiary: fadeout(#827a7a, 50%);\n\n@colorLink: #4A90E2;\n\n// defaults sizes\n@maxContentWidth: 980px;\n\n@gridWidth: 32px;\n@gridHeight: 18.4px;\n\n@lineHeight: @gridHeight*1.2;\n\n@defaultMargin: @gridWidth/2;\n@defaultPaddingVertical: @gridHeight/1.5;\n@defaultPaddingHorizontal: @gridWidth;\n\n// @circleSizeMedium: 40px;\n// @circleSizeLarge: 65px;\n\n@widthContainer: @gridWidth * 30;\n@widthTablet: @gridWidth * 20;\n@widthMobile: 100%;\n\n@heightHeader: @gridWidth * 2;\n@heightHeaderSmall: @gridWidth;\n\n@heightFooter: @gridWidth * 3;\n@heightFooterSmall: @gridWidth * 2;\n\n@widthSidebar: @gridWidth * 7;\n@widthSidebarSmall: @gridWidth * 2;\n\n@widthToolbar: @gridWidth * 2;\n@widthToolbarSmall: @gridWidth;\n\n// fonts\n@fontFamily: 'Source Sans Pro', 'Helvetica Neue', Helvetica, Arial, 'sans-serif';\n\n\n// animations\n@animationSpeed: 400ms;","body {\n    .display(flex);\n    .flex-direction(column);\n    min-height: 100vh;\n    padding: 0;\n    margin: 0;\n    background-color: @colorWhite;\n    font: 300 16px @fontFamily;\n    color: @colorTextPrimary;\n\n    // lock scroll and blur content\n    &.disable-scroll {\n        overflow: hidden;\n    }\n    &.blur {\n        .dapp-flex-content,\n        .dapp-footer,\n        .dapp-header {\n            .blur(4px);\n        }\n    }\n}\n// make font thiner on retina\n@media (-webkit-min-device-pixel-ratio: 2), (min-resolution: 192dpi) {\n    body {\n        font: 100 16px @fontFamily;\n    }\n}\n\n// Special cases on a full screen dapp in mist\n.ethereum-dapp-url-bar-style-transparent {\n    .dapp-header {\n        padding-top: 4 * @gridHeight;\n    }\n}\n\n\n// DEBUG GRID\n.dapp-grid {\n    z-index: 999;\n    background: #FFF url('dapp-styles/hex-grid-tile.png');\n    background-size: 64px 111px;\n    position: absolute;\n    min-height: 100%;\n    left: 0;\n    right: 0;\n    opacity: 0.05;\n    pointer-events: none;\n}\n// body:hover .dapp-grid {\n//     display: none;\n// }\n\n\n\n// CONTAINERS\n\n.dapp-container {\n    display: block;\n    position: relative;\n    margin: 0 auto;\n    max-width: @widthContainer;\n}\n\n// MAIN APP\n\n.dapp-header,\n.dapp-aside,\n.dapp-content,\n.dapp-footer {\n    position: relative;\n    padding: @defaultPaddingVertical @defaultPaddingHorizontal;\n    .box-sizing(border-box);\n}\n\n.dapp-header {\n    .display(flex);\n    .align-items(flex-end);\n    height: @heightHeader;\n    z-index: 50;\n    margin-top: -5px; // necessary to make sure, that its looks good, when blurred\n    padding-top: @defaultPaddingVertical + 5px; \n    padding-bottom: 0;\n    .background-image(linear-gradient(to bottom,  lighten(@colorBackgroundHeader, 15%) 0, @colorBackgroundHeader 90%));\n    .box-sizing(content-box);\n    .user-select(none);\n\n    &.dapp-small {\n        height: @gridHeight * 1.5;\n\n        nav li a i {\n            .opacity(0);\n        }\n    }\n\n    &> .dapp-flex-item {\n        .flex(1);\n    }\n\n    // title\n    .dapp-title {\n        position: absolute;\n        top: @defaultMargin/2;\n        right: @defaultMargin;\n        text-align: right;\n    }\n    hgroup.dapp-title {\n        h1, h2 {\n            margin: 0\n        }\n        h2 {\n            padding: 0;\n            background-color: transparent;\n            color: @colorTextSecondary;\n            text-transform: none;\n            font-family: @fontFamily;\n            font-weight: 300;\n        }\n    }\n    h1.dapp-title {\n        top: @defaultMargin*1.2;\n        margin: 0;\n    }\n\n    // nav\n    nav {\n        width: 100%;\n\n        &> ul {\n            .dapp-horizontal-menu;\n            .display(flex);\n            .align-items(flex-end);\n\n            &> li {\n                flex: 0 1 auto;\n                margin-bottom: 0;\n\n                &> a {\n                    display: inline-block;\n                }\n\n                &.active a,\n                &.active button,\n                button.active,\n                a.active {\n                    color: darken(@colorBackgroundHeader, 20%);\n                    border-bottom: 5px solid @colorWhite;\n                }\n            }\n\n\n            a,\n            a:visited,\n            button {\n                padding: @defaultPaddingVertical/2 @defaultPaddingHorizontal;\n                text-align: center;\n                border-bottom: 5px solid transparent;\n                color: darken(@colorLink, 10%);\n                .transform-origin(50% 100%);\n\n                text-transform: uppercase;\n                font-weight: 400;\n\n                &:focus {\n                    border-color: @colorWhite;\n                }\n                &:active {\n                    // border-bottom-color: transparent;\n                }\n\n                i {\n                    font-size: 1.5em;\n\n                    // ANIMATION\n                    .transition(opacity @animationSpeed / 2);\n                }\n\n                span {\n                    display: block;\n                }\n\n            }\n        }\n    }\n}\n\n.dapp-footer {\n    height: @heightFooter;\n    background-color: @colorBackgroundFooter;\n}\n\n// Makes the content with asides align in rows\n.dapp-flex-content {\n    .display(flex);\n    // .flex-direction(column);\n    .flex(1);\n    // max-height: 100vh;//85vh;\n    background: @colorWhite;\n\n}\n\n.dapp-content {\n    .flex(1);\n    max-width: @maxContentWidth;\n    margin: 0 auto;\n\n    &.dapp-has-header {\n        padding-top: 120px + 3 * @defaultPaddingVertical;\n    }\n\n    .dapp-content-header {\n        position: fixed;\n        top: 0;\n        width: 80%;\n        min-height: @gridHeight * 2;\n        padding: @defaultPaddingVertical @gridWidth / 2;\n        margin-left: -@defaultPaddingHorizontal;\n        background: fadeout(@colorGrayLight, 20%);\n        z-index: 10;\n        line-height: @gridHeight * 2;\n        .dapp-shadow-medium;\n        .box-sizing(border-box);\n    }\n\n    .row {\n        margin-left: -10px;\n        margin-right: -10px;\n    }\n}\n\n\n.dapp-aside {\n    .flex(0 0 @widthSidebar);\n    padding-right: 0;\n    background-color: @colorBackgroundAside;\n\n\n    // ANIMATION\n    .transition(flex @animationSpeed);\n\n    nav {\n        ul {\n            .dapp-vertical-menu;\n            padding-top: @gridHeight*1;\n\n            li {\n\n\n                &.active a,\n                &.active button,\n                a.active,\n                button.active {\n                    background-color: @colorWhite;\n                    border-top: none;        \n                    color: @colorBlack; \n                    font-weight: 500;\n\n                    &:focus {\n                        background-color: fadeout(@colorWhite, 10%);\n                    }\n                }\n\n                // remove the top border\n                &:first-child a,\n                &:first-child button,\n                &.active + li > a,\n                &.active + li > button,\n                + li > a.active,\n                + li > button.active,\n                &.dapp-main-button + li  {\n                    border-top: 0;\n                }\n\n                a,\n                a:visited,\n                button {\n                    .display(flex);\n                    .align-items(center);\n                    position: relative;\n                    width: 100%;\n                    min-height: @gridHeight*4;\n                    max-height: @gridHeight*5;\n                    padding: @defaultPaddingVertical @defaultPaddingHorizontal;\n                    padding-left: @gridWidth / 3;\n                    overflow: hidden;\n                    border-top: darken(@colorBackgroundAside, 8%) solid 1px;\n                    .border-radius(3px 0 0 3px);\n                    .box-sizing(border-box); // align button and links properties\n                    color: @colorForegroundAside;\n\n\n                    // .dapp-shorten-text;\n                    font-weight: 300;\n                    line-height: 20px;\n\n                    &:focus {\n                        background-color: fadeout(@colorWhite, 90%);\n                        border-bottom: 0;\n                    }\n                    &:active {\n                        .transform-origin(100% 50%);\n                        .scale(0.98);\n                    }\n\n                    > i {\n                        margin-right: 4px;\n                    }\n\n                    // elipsis span\n                    > span {\n                        max-width: 115px;\n                        display: -webkit-box;\n                        -webkit-line-clamp: 3;\n                        -webkit-box-orient: vertical;\n                        overflow: hidden;\n                        text-overflow: ellipsis;\n                    }\n\n                }\n                .dapp-main-button {\n                    position: relative;\n                    width: 100%;\n                    margin-bottom: 4*@gridHeight;\n                    .border-radius(3px 0 0 3px);\n                    background: @colorBackgroundMainButton;\n                    color: @colorForegroundMainButton;\n                    border-top: none;\n\n                    \n                    i {\n                        position: absolute;\n                        right: @gridWidth / 4;\n                        top: @gridHeight * 1.5;\n                    }\n                }\n            }\n        }\n    }\n}\n\n.dapp-actionbar {\n    z-index: 20;\n    .flex(0 0 @widthToolbar);\n    background: @colorWhite;\n    margin-left: @gridWidth/2;\n\n    nav {\n        ul {\n        \t.dapp-vertical-menu;\n            \n            li {\n                margin: @defaultMargin 0;// thats what I always use, so we can better control the the spacing, without comming back here. @gridHeight*2 2px 0;\n                color: @colorLink;\n                position: relative;\n                text-align: 